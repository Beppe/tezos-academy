    "cypress": "^4.5.0",
    "eslint-config-react-app": "^5.2.1",
    "mockdate": "^3.0.2",
    "raw-loader": "^4.0.1",
    "react-test-renderer": "^16.13.1",
    "source-map-explorer": "^2.4.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
