{
  "name": "tezosacademy-frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "PORT=3000 NODE_ENV=development react-scripts -r @cypress/instrument-cra start && open http://localhost:3000",
    "build": "PORT=80 NODE_ENV=production react-scripts build",
    "test": "react-scripts test",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:test": "cypress run -s 'cypress/integration/**/*-spec.js'",
