    "connected-react-router": "^6.8.0",
    "dayjs": "^1.8.26",
    "history": "^4.10.1",
    "jwt-decode": "^2.2.0",
    "markdown-to-jsx": "^6.11.4",
    "rc-slider": "9.2.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-google-recaptcha-v3": "^1.4.1",
    "react-helmet": "^6.0.0",
    "react-motion": "^0.5.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-transition": "^2.0.0",
