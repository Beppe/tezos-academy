    "@types/jwt-decode": "^2.2.1",
    "@types/markdown-to-jsx": "^6.11.0",
    "@types/mongodb": "^3.5.14",
    "@types/node": "^13.13.4",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^6.0.0",
    "@types/react-motion": "^0.0.29",
